<!DOCTYPE html>
<html>
  <head>
    <title> <%= full_title(yield(:title)) %> </title>
    <%= stylesheet_link_tag    "application", media: "all"%>
    <%= javascript_include_tag "application"%>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body class="welcome">
    <%- render 'layouts/header' %>
    <div class="container-fluid welcome">
      <div class="row-fluid">
        <div class="span12">
          Level 1 of column
          <div class="row-fluid">
            <div class="span6">Level 2</div>
            <div class="span6">Level 2</div>
          </div>
        </div>
      </div>
    </div>

    <div class="container-fluid div1">
      <div class="container">
        <div class="row valueProps">
          <div class="span12">
            <div class="row-fluid">
              <div class="valuePropBox span4"> hello2 </div>
              <div class="valuePropBox span4">hello4</div>
              <div class="valuePropBox span4"> hello2 </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="container-fluid div2">
      <div class="container">
        <div class="row valueProps">
          <div class="span12">
            <div class="row-fluid">
              <div class="valuePropBox span4"> hello2 </div>
              <div class="valuePropBox span4">hello4</div>
              <div class="valuePropBox span4"> hello2 </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="container-fluid div3">
      <div class="container">
        <div class="row valueProps">
          <div class="span12">
            <div class="row-fluid">
              <div class="valuePropBox span4"> hello2 </div>
              <div class="valuePropBox span4">hello4</div>
              <div class="valuePropBox span4"> hello2 </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <% render partial: 'layouts/flash', locals: {:flash => flash}%>
    <%- yield %>
    <%-debug(params) if Rails.env.development? %>
    <%-debug(flash) if Rails.env.development? %>
    <%-debug(flash.now) if Rails.env.development? %>
    <%-debug(session) if Rails.env.development? %>
    <%-debug(assigns) if Rails.env.development? %>
  </body>
  <%- render 'layouts/footer' %>
</html>
